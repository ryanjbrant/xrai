{
  "name": "portals_raw",
  "version": "1.0.0",
  "main": "index.ts",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "npm run prebuild && expo start --scheme portals",
    "tunnel": "npm run prebuild && expo start --dev-client --tunnel --scheme portals",
    "android": "npm run prebuild && expo run:android",
    "ios": "npm run prebuild && expo run:ios",
    "ios:editor": "bash scripts/build_and_run_ios.sh --skip-unity-export --skip-preflight",
    "ios:preflight": "bash ./scripts/build_and_run_ios.sh --build-only",
    "web": "expo start --web",
    "setup": "export LANG=en_US.UTF-8 && export LC_ALL=en_US.UTF-8 && (command -v git-lfs >/dev/null 2>&1 || { echo >&2 'git-lfs is required. Install with brew install git-lfs'; exit 1; }) && git lfs install && git lfs pull && (command -v pod >/dev/null 2>&1 || { echo >&2 'CocoaPods is required but not installed. Install with brew install cocoapods'; exit 1; }) && ( [ -f .env ] || cp .env.example .env ) && npm install && npx expo prebuild --platform ios --clean && cd ios && pod install && cd .. && ./scripts/patch-fabric-registry.sh",
    "build:unity:ios": "bash scripts/build_and_run_ios.sh --build-only",
    "build:unity:android": "bash scripts/build_and_run_android.sh --build-only",
    "postinstall": "patch-package && ./scripts/patch-virokit.sh && node scripts/patch-react-native-unity.js && ./scripts/patch-rn-unity-message-queue.sh",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/ --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src/ --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "prepare": "husky",
    "prebuild": "node scripts/generate_registry.cjs",
    "setup:ai": "./scripts/setup-ai-context.sh --check",
    "onboard:ai": "npm run setup && npm run verify:ai",
    "kb": "./scripts/kb-search.sh",
    "kb:access": "./scripts/kb-access.sh",
    "kb:fix": "./scripts/kb-cli.sh fix",
    "verify:ai": "./scripts/verify-ai-token-setup.sh",
    "sync:ai": "./scripts/ai-sync.sh",
    "test:editor": "node scripts/test_editor_playmode.js",
    "scout": "node scripts/scout_patterns.cjs",
    "auto-pilot": "./scripts/auto_pilot.sh"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.json": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@artmajeur/react-native-unity": "^0.0.6",
    "@aws-sdk/client-s3": "^3.948.0",
    "@aws-sdk/s3-request-presigner": "^3.948.0",
    "@expo/vector-icons": "^15.0.3",
    "@fal-ai/client": "^1.7.2",
    "@google/generative-ai": "^0.24.1",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-camera-roll/camera-roll": "^7.10.2",
    "@react-native-community/slider": "^5.1.2",
    "@react-navigation/bottom-tabs": "^7.8.12",
    "@react-navigation/native": "^7.1.25",
    "@react-navigation/native-stack": "^7.8.6",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@reactvision/react-viro": "2.43.6",
    "buffer": "^6.0.3",
    "create-react-class": "^15.7.0",
    "dotenv": "^17.2.3",
    "expo": "~54.0.31",
    "expo-asset": "~12.0.12",
    "expo-audio": "~1.1.1",
    "expo-auth-session": "~7.0.10",
    "expo-blur": "~15.0.8",
    "expo-camera": "~17.0.10",
    "expo-clipboard": "~8.0.8",
    "expo-constants": "^18.0.11",
    "expo-crypto": "~15.0.8",
    "expo-dev-client": "~6.0.20",
    "expo-document-picker": "~14.0.8",
    "expo-file-system": "^19.0.20",
    "expo-font": "~14.0.10",
    "expo-gl": "^16.0.9",
    "expo-haptics": "~15.0.8",
    "expo-image-picker": "~17.0.9",
    "expo-linear-gradient": "~15.0.8",
    "expo-location": "~19.0.8",
    "expo-status-bar": "~3.0.9",
    "expo-three": "^8.0.0",
    "expo-video": "~3.0.15",
    "expo-video-thumbnails": "~10.0.8",
    "expo-web-browser": "~15.0.10",
    "firebase": "^12.6.0",
    "lottie-react-native": "^7.3.4",
    "prop-types": "^15.8.1",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "~1.11.0",
    "react-native-maps": "1.20.1",
    "react-native-nitro-modules": "^0.31.10",
    "react-native-nitro-screen-recorder": "^0.6.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-share": "^12.2.1",
    "react-native-svg": "15.12.1",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-video": "^6.18.0",
    "react-native-webview": "13.15.0",
    "react-redux": "^9.2.0",
    "react-refresh": "^0.18.0",
    "react-timer-mixin": "^0.13.4",
    "redux": "^5.0.1",
    "three": "^0.166.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@expo/ngrok": "^4.1.3",
    "@react-native-community/cli": "^20.1.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "29.5.14",
    "@types/react": "~19.1.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "babel-preset-expo": "^54.0.8",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "jest": "~29.7.0",
    "lint-staged": "^16.2.7",
    "patch-package": "^8.0.1",
    "prettier": "^3.8.1",
    "react-test-renderer": "19.1.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2"
  }
}
