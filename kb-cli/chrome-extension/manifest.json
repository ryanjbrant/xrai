{
i "manifest_version": 3,

  "name": "KB-CLI Tab Tracker",
  "version": "1.0",
  "description": "Track and send open tabs to KB-CLI knowledge base",
  "permissions": [
    "tabs",
    "storage",
    "alarms",
    "notifications",
    "history",
    "topSites"
  ],
  "host_permissions": [
    "http://localhost:8000/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "options_page": "options.html"
}
